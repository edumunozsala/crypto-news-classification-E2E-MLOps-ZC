    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::223817798831:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::223817798831:role/ecsTaskExecutionRole",
